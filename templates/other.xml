<templateSet group="other">
  <template name="log" value="private static final java.util.logging.Logger log = java.util.logging.Logger.getLogger($CLASS_NAME$.class.getName());" description="Add log line" toReformat="false" toShortenFQNames="true">
    <variable name="CLASS_NAME" expression="className()" defaultValue="" alwaysStopAt="true" />
    <context>
      <option name="JAVA_CODE" value="true" />
    </context>
  </template>
</templateSet>